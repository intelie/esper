<?xml version="1.0" encoding="UTF-8"?><esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="esper-configuration-1-0.xsd">		<event-type alias="MySampleEvent" class="com.mycompany.myapp.MySampleEvent"/>	<event-type alias="MyNoSchemaXMLEvent">		<xmldom-type>			<xmldom-property property-name="element1" xpath="/myevent/element1" type="double"/>		</xmldom-type>	</event-type>		<event-type alias="MySchemaXMLEvent">		<xmldom-type schema-uri="MySchemaXMLEvent.xsd">			<xmldom-property property-name="element1" xpath="/myevent/element1" type="double"/>		</xmldom-type>	</event-type>	<auto-import import-name="com.mycompany.myapp.*"/></esper-configuration>