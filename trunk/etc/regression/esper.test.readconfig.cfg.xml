<?xml version="1.0" encoding="UTF-8"?><esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../esper-configuration-1-0.xsd">		<event-type alias="MySampleEventOne" class="com.mycompany.myapp.MySampleEventOne"/>	<event-type alias="MySampleEventTwo" class="com.mycompany.myapp.MySampleEventTwo"/>	<event-type alias="MyNoSchemaXMLEventAlias">		<xml-dom root-element-name="MyNoSchemaEvent" >			<xpath-property property-name="element1" xpath="/myevent/element1" type="number"/>		</xml-dom>	</event-type>		<event-type alias="MySchemaXMLEventAlias">		<xml-dom root-element-name="MySchemaEvent" root-element-namespace="samples:schemas:simpleSchema" schema-resource="MySchemaXMLEvent.xsd" 				default-namespace="default-name-space">			<namespace-prefix prefix="ss" namespace="samples:schemas:simpleSchema"/>			<xpath-property property-name="element1" xpath="/myevent/element1" type="number"/>		</xml-dom>	</event-type>	<auto-import import-name="com.mycompany.myapp.*"/>	<auto-import import-name="com.mycompany.myapp.ClassOne"/></esper-configuration>