<?xml version="1.0" encoding="UTF-8"?><esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../etc/esper-configuration-1-0.xsd">	<event-type alias="StockTick" class="net.esper.example.stockticker.eventbean.StockTick"/>	<event-type alias="PriceLimit" class="net.esper.example.stockticker.eventbean.PriceLimit"/>		<event-type alias="AutoIdRFIDExample">		<xml-dom root-element-name="Sensor" schema-resource="data/AutoIdPmlCore.xsd" default-namespace="urn:autoid:specification:interchange:PMLCore:xml:schema:1">			<namespace-prefix prefix="pmlcore" namespace="urn:autoid:specification:interchange:PMLCore:xml:schema:1"/>			<xpath-property property-name="countTags" xpath="count(/pmlcore:Sensor/pmlcore:Observation/pmlcore:Tag)" type="number"/>		</xml-dom>	</event-type>	<auto-import import-name="java.lang.Double"/>	<auto-import import-name="java.math.*"/></esper-configuration>